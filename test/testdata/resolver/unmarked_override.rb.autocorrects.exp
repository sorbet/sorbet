# -- test/testdata/resolver/unmarked_override.rb --
# typed: true
# enable-suggest-unsafe: true

class A
  extend T::Sig

  sig { returns(Integer) }
  def foo; 0; end
end

class B < A
  extend T::Sig

  sig { override(allow_incompatible: true).returns(String) }
  def foo; ""; end
# ^^^^^^^ error: Return type `String` does not match return type of overridden method `A#foo`
end

# No sig is untyped
class C < A
  def foo; ""; end
end

class D < A
  extend T::Sig

  sig { override(allow_incompatible: true).params(x: Integer).returns(Integer) }
  def foo(x)
# ^^^^^^^^^^ error: Override of method `A#foo` must accept no more than `0` required argument(s)
    0
  end
end

class E < A
  extend T::Sig

  sig { override(allow_incompatible: true).returns(Integer).params(x: Integer) }
  def foo(x)
# ^^^^^^^^^^ error: Override of method `A#foo` must accept no more than `0` required argument(s)
    0
  end
end

class F < A
  extend T::Sig
  extend T::Helpers

  abstract!

  sig { override(allow_incompatible: true).void.abstract }
  def foo; end
# ^^^^^^^ error: Return type `void` does not match return type of overridden method `A#foo`
end

class G < A
  extend T::Sig

  sig { override(allow_incompatible: :visibility).returns(Integer) }
  private def foo; 0; end
#         ^^^^^^^ error: Method `foo` is private in `G` but not in `A`
end
# ------------------------------
