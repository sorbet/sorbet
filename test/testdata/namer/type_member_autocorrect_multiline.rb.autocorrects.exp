# typed: true
extend T::Sig

class Example
  extend T::Sig
  extend T::Generic

  # Also need to delete the double space in other changes: `type_member  {{...}}`

  A1 = type_member do
    {fixed: Integer}
  end
  A2 = type_member do
    {
      lower: Integer,
      upper: T.any(Integer, String),
    }
  end

  B1 = type_member(
    :out,
   
  ) {{fixed: Integer}}
  B2 = type_member(
    :out,
   
  ) {{lower: Integer,
    upper: T.any(Integer, String)}}

  C1 = type_member do
    {
      fixed: T.any(
        Integer,
        String
      ),
    }
  end
  C2 = type_member(:out) {{fixed: T.any(
    Integer,
    String
  )}}
end
