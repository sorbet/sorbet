  end

  def self.no_sig_no_block; end

  sig { void }
  def self.void_no_block; end

  sig { params(blk: T.noreturn).void }
  def self.noreturn_no_block(&blk); end

  sig { params(blk: NilClass).void }
  def self.nilclass_no_block(&blk); end
end

