module One
  FOO = 1
end
module Two
  FOO = 2
end
module Three
  FOO = 3
end
class Base
  include Three
  FOO = 4
end


class OneTwo < Base
  include One
  include Two
  FOO # Should resolve to Two::FOO
end

class TwoOne < Base
  include Two
  include One
  FOO # Should resolve to One::FOO
end

